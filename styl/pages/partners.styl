partners-colors = {
  underline: #ffc21d
  number: #ffc21d
  row-border: #cfcfcf
  text-grey: #8b7b7f
}

body
  margin: 0

.container-content.page-partners
  margin: 0 auto
  width: 960px
  .top-slider
    margin: 0
    background: url(imgPath + 'partners_banner.png') no-repeat;
    width: 1300px
    background-size: 100%
    height: 484px
    .title
      font-size: 40px
      margin: 0
      padding-top: 170px
      boldFont()
    .subtitle
      width: 565px
      margin: 0
      font-size: 30px
      margin: 0
      mediumFont()
    .btn.resellers
      width: 180px
      display: block
      margin: 40px 0 0 0
      background: url("../img/btn-sprite.jpg") no-repeat
      background-position: -70px -40px
      color: colors.darkText

.partners-section-name
  font-size: 24px
  boldFont()
  margin: 40px 0

.slide-description
  clearfix()
  p
    margin: 0
  .partners-first-item
    height: 100px
    width: 380px
    float: left
    p
      width: 300px
  .partners-second-item
    height: 100px
    width: 580px
    float: left



.host-advantages,
.profit,
.discount-and-amount,
.partnership
  width: 960px
  margin: 0 auto
  clearfix()
  .title-main
    font-size: 30px
    mediumFont()
    span
      border-bottom: 4px solid partners-colors.underline
      boldFont()
  .advantage-item-sm
    margin: 65px 0 0 0
    width: 300px
    position: relative
  .advantage-item-md
    position: relative
  .item-title
    font-size: 18px
    boldFont()
    margin: 0px 0 27px 0
  .advantages-col-sm
    width: 380px
    float: left
  .advantages-col-md
    width: 580px
    float: left
  .templates-gallery
    // background: url(imgPath + 'templates.png') no-repeat
    height: 180px
    margin: 65px 0 65px 0
    position: relative
    .btn
      display: block
      width: 190px
      position: absolute
      padding-left: 13px
      top: 118px
      right: -15px
      text-transform: uppercase
      background: url("../img/btn-sprite.jpg") no-repeat 
      background-position: -71px -40px
      color: colors.darkText 
      z-index: 200
      &:hover
        background-position: -71px 0
    > div
      position: absolute
      border: 1px solid black
    .first-template
      background: url(imgPath + 'cover-assets/1.jpg') no-repeat
      width: 240px
      height: 180px
      z-index: 100

      border-right: 3px solid #231f20
    .second-template
      width: 230px
      height: 172px
      z-index: 99
      left: 22%
      top: 8px
      background: -moz-linear-gradient(top,  rgba(35,31,32,0.3) 0%, rgba(35,31,32,0.8) 100%), url(imgPath + 'cover-assets/2.jpg') no-repeat
      background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(35,31,32,0.3)), color-stop(100%,rgba(35,31,32,0.8))), url(imgPath + 'cover-assets/2.jpg') no-repeat
